{
  "name": "trabalho-api",
  "version": "1.0.0",
  "description": "API Rest para aprendizado de testes e automação",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "mocha test/**/*.test.js --timeout 10000",
    "test:integration": "mocha test/integration/*.test.js --timeout 10000",
    "test:watch": "mocha test/**/*.test.js --watch --timeout 10000",
    "test:coverage": "nyc mocha test/**/*.test.js --timeout 10000"
  },
  "keywords": [
    "api",
    "express",
    "rest",
    "swagger"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "sinon": "^21.0.0",
    "supertest": "^6.3.3"
  }
}
